<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>

<div class="flex-container">
{{#each movies}}
<div>
<ul class="all-movies">
    <img src={{image}} alt={{title}}>
    <li id="movie-title">{{title}}</li>
    <a href="/movies/{{_id}}">See more</a>
</ul>
</div>
{{/each}}
</div>